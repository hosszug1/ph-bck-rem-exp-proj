# Environment variables for the background remover service
# Copy this file to .env and set your actual values

# Photoroom API Key (required)
PHOTOROOM_API_KEY=your_photoroom_api_key_here
PHOTOROOM_API_URL=https://api.photoroom.com

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=True

# Prefect Configuration (for Docker Compose)
PREFECT_API_URL=http://prefect-server:4200/api

# For local development (when running without Docker)
# PREFECT_API_URL=http://localhost:4200/api

